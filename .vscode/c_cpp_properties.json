{
    "configurations": [
        {
            "name": "Arduino-ServoDAC-Win",
            "includePath": [
                "${workspaceFolder}/src",
                "${env:ARDUINO_LIB}/GPIOUtils/src",
                "${env:ARDUINO_LIB}/LiquidCrystal_I2C",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/libraries/**",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/cores/arduino",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/variants/standard",
                "${env:ARDUINO15}/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/avr/include"
            ],
            "defines": [
                "F_CPU=16000000L",
                "ARDUINO"
            ],
            "compilerPath": "${env:ARDUINO15}/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-g++.exe",
            "compilerArgs": [
                "-x",
                "c++",
                "-std=gnu++11",
                "-mmcu=atmega328p"
            ],
            "intelliSenseMode": "windows-gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:ARDUINO_LIB}",
                    "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/cores/arduino",
                    "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/libraries/Wire/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}